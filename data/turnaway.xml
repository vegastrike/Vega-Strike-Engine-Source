<SCRIPT>
 <MatchLin afterburn="1" local="0">
   <SCALE>
     <Float value="200"/>
     <SUB>
        <Vector Position="v"/>
        <Vector TargetPos="u"/>
     </SUB>
   </SCALE>
   <Vector Dup="200*(v - u)"/>
 </MatchLin>
 <ExecuteFor time="2">
  <ChangeHead>
   <ADD>
    <Vector TargetPos="u"/>
   </ADD>
  </ChangeHead>
 </ExecuteFor>
</SCRIPT>
